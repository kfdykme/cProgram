#include <stdio.h>

int main(void){
	int min, max, a, b, c, d;
	
	printf("Enter four intgers(a b c d): ");
	scanf("%d %d %d %d", &a, &b, &c ,&d);
	
	max = a;
	min = a;
	if(max <  b){
		max = b;
	} ;
	if(max < c){
		max = c;
	};
	if(max < d){
		max = d;
	};
	if(min >   b){
		min = b;
	} ;
	if(min >  c){
		min = c;
	};
	if(min >  d){
		min = d;
	};
	printf("Largest: %d\nSmallest: %d",max,min);
			
	return 0;
	
}
		 