//不明白题目的意思

#include <stdio.h>

#define L (int)(sizeof(a) / sizeof(a[0]))

int main(void){
	int a[10],i;
	
	printf("Enter %d numbers(格式： a b c d ... x): ",L);
	for ( i = 0; i < L; i++)
		scanf("%d",&a[i]);
	
	printf("In reverse order: ");
	for ( i = L - 1; i >= 0; i--)
		printf(" %d", a[i]);
	printf("\n");
	
	return 0;
}